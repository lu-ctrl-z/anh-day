<div id="head-container">
    <div class="head-offset-inner bg-white border-bottom">
        <div class="_table w100 head-top">
            <div class="td">
               <button type="button" class="op-ux-button" id="button-show-menu">
                   <span class="glyphicons glyphicons-show-lines"></span>
               </button>
            </div>
            <div class="td logo">
                <%if(req.session.organization) {%>
               <a class="home-link" href="/"><img class="logo-image" src="<%=req.session.organization.imagePath || '/image/logo0904.gif'%>" alt="<%=req.session.organization.organizationName%>" title="<%=req.session.organization.organizationName%>"> </a>
                <%}%>
            </div>
            <div class="td head-seach">
               <form method="get">
                   <table>
                       <tr>
                           <td>
                               <input type="search" name="search_query" class="op-ux-input head-search-query">
                           </td>
                           <td>
                               <button type="submit" class="op-ux-button head-search-button">
                                    <span class="glyphicons glyphicons-search"></span>
                               </button>
                           </td>
                       </tr>
                   </table>
               </form>
            </div>
            <div class="td free-col">
            </div>
            <div class="td">
                <button type="button" onclick="printDocument();" class="op-ux-button button-visibled button-active hasCount" id="button-show-menu">
                    <span class="glyphicons glyphicons-print"></span>
                    <span id="peddingCount" style="display: none;" class="count-p"></span>
                </button>
            </div>
            <div class="td" style="padding-left: 20px;">
                <button type="button" onclick="printDocument();" class="op-ux-button button-visibled button-active hasCount" id="button-show-menu">
                    <span class="glyphicons glyphicons-globe-af"></span>
                    <span id="notifyCount" style="display: none;" class="count-p"></span>
                </button>
            </div>
            <div class="td" style="padding-left: 20px;">
                <a title="logout" style="display: inline-block;" href="/logout?_csrf=<%=_csrf%>" class="op-ux-button button-visibled button-active">
                    <span class="glyphicons glyphicons-log-out" style="font-size: 18px;"></span>
                </a>
            </div>
        </div>
    </div>
</div>
<div id="head-height-offset"></div>
<%include _print_modal%>